{
  "name": "comments",
  "version": "1.0.0",
  "description": "comments servierce providers features where a user can comment on reviews and potentially like other user's comments",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "run": "node server.js",
    "test": "node_modules/.bin/jest --coverage --coverageDirectory=docs/coverage --runInBand --NODE_ENV=test --detectOpenHandles",
    "dependency": "node_modules/.bin/dependency-check --unused --no-dev . && node_modules/.bin/dependency-check -i modules --missing .",
    "coverage": "node_modules/.bin/istanbul cover node_modules/.bin/jest --coverage --runInBand --NODE_ENV=test --detectOpenHandles",
    "check-coverage": "node_modules/.bin/istanbul check-coverage --statement 100 --branch 100 --function 100 --line 100"
  },
  "pre-commit": [
    "dependency"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.coventry.ac.uk/340CT-1920SEPJAN/majekodr.git"
  },
  "author": "Rukiyat Majekodunmi",
  "license": "ISC",
  "dependencies": {
    "@koa/cors": "^2.2.3",
    "dotenv": "^8.2.0",
    "koa": "^2.8.2",
    "koa-bodyparser": "^4.2.1",
    "koa-json": "^2.0.2",
    "koa-router": "^7.4.0",
    "mysql": "^2.17.1"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "coverage": "^0.4.1",
    "dependency-check": "^2.9.2",
    "eslint-config-sinon": "^3.0.1",
    "eslint-plugin-local-rules": "^0.1.1",
    "frisby": "^2.0.10",
    "istanbul": "^0.4.5",
    "jasmine": "^2.99.0",
    "jasmine-console-reporter": "^2.0.1",
    "jasmine-node": "^3.0.0",
    "jasmine-supertest": "^1.0.0",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.3",
    "sinon": "^7.5.0",
    "sqlite-async": "^1.0.12",
    "pre-commit": "^1.2.2",
    "supertest": "^4.0.2"
  },
  "jest": {
    "projects": [
      "<rootDir>/jest-test.config.js"
    ]
  }
}
